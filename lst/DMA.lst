N
Nvoid DMA_Init(void) {
N	SIM->SCGC7 |= SIM_SCGC7_DMA_MASK;
X	((SIM_Type *)(0x40047000u))->SCGC7 |= 0x100u;
N	SIM->SCGC6 |= SIM_SCGC6_DMAMUX_MASK;
X	((SIM_Type *)(0x40047000u))->SCGC6 |= 0x2u;
N}
N
Nvoid Configure_DMA_For_Playback(uint16_t * source, uint32_t count, uint32_t num_playbacks) {
N
N/*
N	SIM->SCGC7 |= SIM_SCGC7_DMA_MASK;
N	SIM->SCGC6 |= SIM_SCGC6_DMAMUX_MASK;
N*/
N	
N	// Disable DMA channel, allows changes
N	DMAMUX0->CHCFG[0] = 0;
X	((DMAMUX_Type *)(0x40021000u))->CHCFG[0] = 0;
N
N	Reload_DMA_Source = source;
N	Reload_DMA_Byte_Count = count*2;
N	DMA_Playback_Count = num_playbacks;
N	
N	// Generate DMA interrupt when done
N	// Increment source, transfer words (16 bits)
N	// Enable peripheral request
N	DMA0->DMA[0].DCR = DMA_DCR_EINT_MASK | DMA_DCR_SINC_MASK | 
X	((DMA_Type *)(0x40008000u))->DMA[0].DCR = 0x80000000u | 0x400000u | 
N											DMA_DCR_SSIZE(2) | DMA_DCR_DSIZE(2) |
X											(((uint32_t)(((uint32_t)(2))<<20))&0x300000u) | (((uint32_t)(((uint32_t)(2))<<17))&0x60000u) |
N											DMA_DCR_ERQ_MASK | DMA_DCR_CS_MASK;
X											0x40000000u | 0x20000000u;
N
N	
N	// Configure NVIC for DMA ISR
N	NVIC_SetPriority(DMA0_IRQn, 128); // 0, 64, 128 or 192
N	NVIC_ClearPendingIRQ(DMA0_IRQn); 
N	NVIC_EnableIRQ(DMA0_IRQn);	
N
N	// Enable DMA MUX channel without periodic triggering
N	// select TPM0 overflow as trigger
N	DMAMUX0->CHCFG[0] = DMAMUX_CHCFG_SOURCE(54);   
X	((DMAMUX_Type *)(0x40021000u))->CHCFG[0] = (((uint8_t)(((uint8_t)(54))<<0))&0x3Fu);   
N}
N
Nvoid Start_DMA_Playback() {
N	
N#if 0 // old demo code
S	if (DMA_Playback_Count == 0)
S		return;
N#endif
N	
N	DMAMUX0->CHCFG[0] = DMAMUX_CHCFG_SOURCE(54);   
X	((DMAMUX_Type *)(0x40021000u))->CHCFG[0] = (((uint8_t)(((uint8_t)(54))<<0))&0x3Fu);   
N
N	// initialize source and destination pointers
N	DMA0->DMA[0].SAR = DMA_SAR_SAR((uint32_t) Reload_DMA_Source);
X	((DMA_Type *)(0x40008000u))->DMA[0].SAR = (((uint32_t)(((uint32_t)((uint32_t) Reload_DMA_Source))<<0))&0xFFFFFFFFu);
N	DMA0->DMA[0].DAR = DMA_DAR_DAR((uint32_t) (&(DAC0->DAT[0])));
X	((DMA_Type *)(0x40008000u))->DMA[0].DAR = (((uint32_t)(((uint32_t)((uint32_t) (&(((DAC_Type *)(0x4003F000u))->DAT[0]))))<<0))&0xFFFFFFFFu);
N	
N	// byte count
N	DMA0->DMA[0].DSR_BCR = DMA_DSR_BCR_BCR(Reload_DMA_Byte_Count);
X	((DMA_Type *)(0x40008000u))->DMA[0].DSR_BCR = (((uint32_t)(((uint32_t)(Reload_DMA_Byte_Count))<<0))&0xFFFFFFu);
N	
N	// verify done flag is cleared
N	DMA0->DMA[0].DSR_BCR &= ~DMA_DSR_BCR_DONE_MASK; 
X	((DMA_Type *)(0x40008000u))->DMA[0].DSR_BCR &= ~0x1000000u; 
N	
N	DMAMUX0->CHCFG[0] |= DMAMUX_CHCFG_ENBL_MASK;
X	((DMAMUX_Type *)(0x40021000u))->CHCFG[0] |= 0x80u;
N
N	// start the timer running
N	TPM0_Start();
N}
