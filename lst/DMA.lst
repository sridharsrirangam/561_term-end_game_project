N	// Clear done flag 
N	DMA0->DMA[0].DSR_BCR |= DMA_DSR_BCR_DONE_MASK; 
X	((DMA_Type *)(0x40008000u))->DMA[0].DSR_BCR |= 0x1000000u; 
N
N#if 1 // under development for continuous playback
N	// Signal event requesting source buffer refill
N	isr_evt_set(EV_REFILL_SOUND, t_Refill_Sound_Buffer);
X	isr_evt_set((1), t_Refill_Sound_Buffer);
N	// Start playback again
N	Start_DMA_Playback();
N#else
S	// Disable DMA
S	DMAMUX0->CHCFG[0] &= ~DMAMUX_CHCFG_ENBL_MASK;
N#endif 
N	
N	Control_RGB_LEDs(0,0,0);
